/* 医生列表 */

.staffList {
  display: flex;
  flex-wrap: nowrap;
  justify-content: flex-start;
  overflow-x: scroll;
  align-items: center;
  margin-bottom: 10px;
}

.staffList>.navigator {
  display: flex;
  flex-direction: column;
  font-size: 12px;
  text-align: center;
  align-items: center;
  margin-right: 10px;
}

.staffList .button {
  font-size: 16px;
  width: 106.33px;
  height: 21.33px;
  line-height: 21.33px;
  color: #A38554;
  border-radius: 1em;
  margin-top: 10rpx;
}

/* 广告板 */

.ad-container {
  padding: 0 15px 30px;
