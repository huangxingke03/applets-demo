  bottom: -20px;
  left: 0;
  font-weight: bold;
}

.flex_entry {
  display: flex;
  justify-content: space-around;
  font-size: 13px;
  text-align: center;
  color: rgba(0, 0, 0, 0.8);
  line-height: 1.5;
}

