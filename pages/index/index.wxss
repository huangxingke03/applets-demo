/* news start */

.news {
  padding: 0 10px;
}

.flex_card {
  padding: 10px 0px;
  display: flex;
  border-bottom: 1px solid gainsboro;
}

.news .image {
  height: 77px;
}

.news image {
  width: 100%;
  height: 150rpx;
}

.news .text-content {
  padding-left: 125px;
  height: 72px;
}

.appointmentStatusText {
  border-radius: 6rpx;
  padding: 5rpx 15rpx;
  position: absolute;
  right: 10px;
  top: 10px;
  background: #936727;
}

.appointmentStatusText1 {
  border: 2rpx solid;
  border-radius: 6rpx;
  padding: 0 10rpx;
  display: inline-flex;
  justify-content: flex-end;
  margin-right: 32rpx;
}

.display-flex-row {
  display: flex;
  flex-direction: row;
  width: 100%;
}

.display-flex-column {
  width: 100%;
  display: flex;
  flex-direction: column;
}

.flex-1 {
  display: inline-flex;
  flex: 1;
  /* justify-content: flex-end; */
}

.news .text1 {
  font-size: 25rpx;
  line-height: 1.3;
  margin: 15rpx;
  -webkit-line-clamp: 2;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  text-overflow: ellipsis;
  overflow: hidden;
}

.news .text2 {
  font-size: 11px;
  color: rgba(0, 0, 0, 0.5);
  position: absolute;
  bottom: 11px;
  left: 136px;
  width: 467rpx;
  -webkit-line-clamp: 1;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  text-overflow: ellipsis;
  overflow: hidden;
}

/* news end */

/* 预约 */

.order {
  display: flex;
  align-items: center;
  padding: 10rpx 15rpx;
  background: #fff;
  border-radius: 20rpx;
  margin: 20px;
  position: relative;
}

.border-bottom-view {
  border-bottom: 2rpx solid rgb(217, 218, 220);
  margin: 0 20rpx;
}

.display-flex-row .hr_scale-half_v {
  padding: 2% 0;
  margin: 1.5% 0;
  border-left: 1px solid rgba(69, 90, 100, 0.6);
  width: 2px;
}

.order-left-gray {
  border-left: 1px solid rgba(69, 90, 100, 0.6);
}

.order .text2 {
  font-size: 12px;
  color: rgba(0, 0, 0, 0.8);
}

.card-container {
  width: calc(100vw - 25px);
  height: calc((100vw - 25px) / 10 * 5);
  margin: 15px auto;
  border-radius: 10px;
  color: #fff;
  font-size: 14px;
  padding: 15.34px 0;
  position: relative;
  box-sizing: border-box;
  box-shadow: 6px 6px 14px rgba(195, 190, 186, 1.000);
}

.userinfo-avatar {
  height: 52.33px;
  width: 52.33px;
  margin: 10px 13.4px 13.4px;
  border-radius: 50%;
  background-size: cover;
  background-color: white;
  border: 1px solid #efe3d1;
  overflow: hidden;
}

.userinfo {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  transition: all 300ms ease;
  position: relative;
  padding-left: 28px;
}

.userinfo .primary {
  position: absolute;
  top: 0;
  right: 0px;
  width: 80px;
  height: 28.5px;
  margin-top: 10px;
  font-size: 11.3333px;
  display: flex;
  align-items: center;
  border-radius: 16px 0 0 16px;
  background: #bf7e27;
  justify-content: space-around;
}

.userinfo .primary .text-content {
  z-index: 2;
}

.userinfo .primary text {
  padding: 0 0 0 1em;
  font-size: 11.4px;
}

.userinfo .text1 {
  margin-left: 0rpx;
  line-height: 1;
  padding: 0 0.5em 11.4px 0.2em;
  border-bottom: 1px dotted #efe3d1;
  font-size: 11.4px;
  display: inline-block;
}

.pin {
  position: relative;
  line-height: 26.3333px;
  height: 26.3333px;
  font-size: 15px;
  color: black;
}

.pin-view {
  display: flex;
  justify-content: space-between;
  padding-left: 28px;
}

.pin text {
  z-index: 3;
  position: relative;
}

.pin .container {
  display: flex;
  align-items: center;
  position: absolute;
  top: 0;
  left: 0
}

.pin .star {
  height: 26.33333px;
  width: 26.333333px;
  left: 0;
  top: 0;
  z-index: 3;
  position: relative
}

.pin .bar {
  margin-left: -26.3333px;
  height: 18px;
  width: calc(26.33px + 4em + 1.5em);
  position: relative;
  z-index: 2
}

.date {
  padding-right: 22px;
  font-size: 12px;
  line-height: 26.3333px;
  height: 26.3333px;
  font-size: 15px;
  color: black;
  opacity: 0.8;
}

